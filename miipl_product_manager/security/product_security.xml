<?xml version="1.0" encoding="UTF-8"?>
  <openerp>
	<data>
        <record model="ir.module.category" id="module_general_product_category">
			<field name="name">Product</field>
			<field name="Sequence">2</field>
		</record>

        <record id = "miipl_product_manager" model="res.groups">
			<field name="name">Product Manager</field>
			<field name="category_id" ref="module_general_product_category"/>
		</record>

		<record id = "miipl_product_manager_user" model="res.groups">
			<field name="name">Product User</field>
			<field name="category_id" ref="module_general_product_category"/>
		</record>

		<record model='ir.ui.menu' id='main_menu_miipl_products'>
			<field name="groups_id" eval="[(6,0,[ref('miipl_product_manager'),ref('miipl_product_manager_user')])]"/>
		</record>


     </data>

    </openerp>